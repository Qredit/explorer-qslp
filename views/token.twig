{% extends '/subblocks/main.twig' %}

{% block body %}
{% include '/subblocks/menu.twig' %}
{% include '/cards/heading1.twig' %}


<div class="container" style="margin-top:50px;margin-bottom:50px;">
	<div class="container">

		<div class="banner-servers-box">
			<div class="counter-placeholder"></div>
			<div class="banner-text-left">
				<h4 style="color:#fff;">
					<strong>QSLP Tokens</strong>
				</h4>
				<p>QSLP tokens on the Qredit Blockchain</p>
			</div>
		</div>

		<div class="table-responsive">

			<table class="table">


				<thead>
					<tr>
						<th class="server-tabls-head" scope="col">Data</th>
						<th class="server-tabls-head" scope="col">Output</th>
					</tr>
				</thead>
				<tbody id='tokeninfo' class="server-tabls-row"></tbody>


			</table>
		</div>
	</div>
</div>
</div>
</div>
</div>{% include '/subblocks/footer.twig' %}
</div{{tokenid}} {% endblock %} {% block javascripts %} <script>
	$(document).ready(function () {



		var socket = io();

		socket.emit('gettokeninfo', { tokenid: '{{ tokenid }}' });

		socket.on('showtokeninfo', function (data) {

			var mainTable = document.getElementById("tokeninfo");
			var tr, td;

			data.forEach(function (row) {

				tr = document.createElement("tr");

				td = document.createElement("td");
				td.innerHTML = row.symbol;
				tr.appendChild(td);

				td = document.createElement("td");
				td.innerHTML = row.paused;
				tr.appendChild(td);

				tokeninfo.appendChild(tr);

			});
		});
	});
</script>


{% endblock %}